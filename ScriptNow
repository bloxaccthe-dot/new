local function formatTimeAgo(seconds)
   if seconds < 60 then
      return string.format("Ù‚Ø¨Ù„ %d Ø«Ø§Ù†ÙŠØ©", seconds)
   elseif seconds < 3600 then
      return string.format("Ù‚Ø¨Ù„ %d Ø¯Ù‚ÙŠÙ‚Ø©", math.floor(seconds / 60))
   elseif seconds < 86400 then
      return string.format("Ù‚Ø¨Ù„ %d Ø³Ø§Ø¹Ø©", math.floor(seconds / 3600))
   else
      return string.format("Ù‚Ø¨Ù„ %d ÙŠÙˆÙ…", math.floor(seconds / 86400))
   end
end

