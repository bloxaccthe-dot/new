   Options = GetPlayerNames(),
   CurrentOption = "",
   Flag = "TeleportDropdown",
   Callback = function(Option)
      SelectedPlayer = Option
      Tab:CreateNotification({
         Name = "ğŸ“Œ Ù„Ø§Ø¹Ø¨ Ù…Ø­Ø¯Ø¯",
         Content = "Ø§Ø®ØªØ±Øª Ø§Ù„Ù„Ø§Ø¹Ø¨: " .. Option,
         Time = 3
      })
   end,
})

