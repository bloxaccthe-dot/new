      return string.format("Ù‚Ø¨Ù„ %d %s", m, (m == 1 and "Ø¯Ù‚ÙŠÙ‚Ø©" or "Ø¯Ù‚Ø§Ø¦Ù‚"))
   elseif seconds < 86400 then
      local h = math.floor(seconds / 3600)
      return string.format("Ù‚Ø¨Ù„ %d %s", h, (h == 1 and "Ø³Ø§Ø¹Ø©" or "Ø³Ø§Ø¹Ø§Øª"))
   else
