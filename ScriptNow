      return string.format("Ø§Ù„Ø³ÙŠØ±ÙØ± ÙŠØ¹Ù…Ù„ Ù…Ù†Ø° %d Ø¯Ù‚ÙŠÙ‚Ø©", minutes)
   else
      return string.format("Ø§Ù„Ø³ÙŠØ±ÙØ± ÙŠØ¹Ù…Ù„ Ù…Ù†Ø° %d Ø«Ø§Ù†ÙŠØ©", math.floor(seconds))
   end
end

