   local minutes = math.floor((seconds % 3600) / 60)

   if hours > 0 then
      return string.format("Ø§Ù„Ø³ÙŠØ±ÙØ± ÙŠØ¹Ù…Ù„ Ù…Ù†Ø° %d Ø³Ø§Ø¹Ø© Ùˆ %d Ø¯Ù‚ÙŠÙ‚Ø©", hours, minutes)
